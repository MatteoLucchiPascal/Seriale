<Window x:Class="Seriale.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Label x:Name="lbl1Stampa" Content="" HorizontalAlignment="Left" Margin="305,105,0,0" VerticalAlignment="Top" Width="175"/>
        <ComboBox x:Name="cbSerialPort" HorizontalAlignment="Left" Margin="75,46,0,0" VerticalAlignment="Top" Width="120"/>
        <ComboBox x:Name="cbSerialSpeed" HorizontalAlignment="Left" Margin="239,46,0,0" VerticalAlignment="Top" Width="120"/>
        <Button HorizontalAlignment="Left" Margin="429,68,0,0" VerticalAlignment="Top" Width="75">
            <Button x:Name="btnOpenPort" Content="Open" Width="75" Click="btnOpenPort_Click"/>
        </Button>
        <TextBox x:Name="txtMsg" HorizontalAlignment="Left" Height="20" Margin="18,209,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="341"/>
        <Button x:Name="btn_Close" Content="Close" HorizontalAlignment="Left" Margin="36,90,0,0" VerticalAlignment="Top" Width="75" Click="btn_Close_Click"/>
        <Label x:Name="lbl1" Content="Label" HorizontalAlignment="Left" Margin="90,270,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
